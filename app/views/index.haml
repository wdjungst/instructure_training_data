!!!
%html
  %head
    <link href="/css/training.css" rel="stylesheet" type="text/css">
    <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/css/bootstrap-responsive.min.css" rel="stylesheet" type="text/css">
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}/
    %title
      Instructure Training Dept CAT Feedback
    %meta{:content => "", :name => "description"}/
    %meta{:content => "width=device-width", :name => "viewport"}/
    :css
      body {
        padding-top: 60px;
        padding-bottom: 40px;
      }
    %link{:href => "css/base.min.css", :rel => "stylesheet"}/
    %link{:href => "css/jquery.jqplot.css", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "/javascripts/modernizr-2.6.2-respond-1.1.0.min.js"}
  %body
    .content-wrapper
      .row-fluid.span12
        .page-header.title 
          %h1 
            CAT Training Outlines Feedback Activity
            %small App by David Jungst
        .tabbable.tabs-left
          %ul.nav.nav-tabs
            %li.active#week0
              %a{:href => "#course_total", "data-toggle" => "tab"} Course Total
            %li#week1
              %a{:href => "#week1", "data-toggle" => "tab"} Week 1
            %li#week2
              %a{:href => "#week2", "data-toggle" => "tab"} Week 2
            %li#week3
              %a{:href => "#week3", "data-toggle" => "tab"} Week 3
            %li#week4
              %a{:href => "#week4", "data-toggle" => "tab"} Week 4
            %li#week5
              %a{:href => "#week5", "data-toggle" => "tab"} Week 5
            %li#week6
              %a{:href => "#week6", "data-toggle" => "tab"} Week 6
            %li#week7
              %a{:href => "#week7", "data-toggle" => "tab"} Week 7
            %li#week8
              %a{:href => "#week8", "data-toggle" => "tab"} Week 8
          .tab-content
            .tab-pane.active#course_total
              .graph-container
            .tab-pane#week1
              .graph-container
            .tab-pane#week2
              .graph-container
            .tab-pane#week3
              .graph-container
            .tab-pane#week4
              .graph-container
            .tab-pane#week5
              .graph-container
            .tab-pane#week6
              .graph-container
            .tab-pane#week7
              .graph-container
            .tab-pane#week8
              .graph-container
        .container        
          .btn-group#users-btn
            %button.btn.btn-primary#modal-btn View By User
          %h4 *Data is updated every 24 hours.       
        .modal.hide.fade#users-modal
          .modal-header
            %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", :type => "button"} X
            %h3 Week By Week Analysis
            .label.label-success.large#user-label Select User
          .modal-body
            .modal-container
              .btn-group.dropdown#modal-users
                %button.btn.dropdown-toggle{"data-toggle" => "dropdown"} Users
                %button.btn.dropdown-toggle{"data-toggle" => "dropdown"}
                  %span.caret
                %ul.dropdown-menu.scroll-menu#modal_dropdown
              #chartdiv
          .modal-footer
            %button.btn.btn-primary{"aria-hidden" => "true", "data-dismiss" => "modal"} Close
    %script{:src => "//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/jquery.jqplot.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/jqplot.canvasTextRenderer.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/jqplot.canvasAxisTickRenderer.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/jqplot.categoryAxisRenderer.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/jqplot.highlighter.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/bootstrap.min.js"}
    %script{:type => "text/javascript", :src => "/javascripts/jqBarGraph.1.1.js"}
    %script{:type => "text/javascript", :src => "/javascripts/training.js"}
